  object ReJournalPos: TZQuery
    Connection = DM1.DB1
    OnCalcFields = JournalTabCalcFields
    SQL.Strings = (
      'select'
      '(Round(GPreis+(GPREIS*registry.VAL_DOUBLE/100),2)) as Summe'
      
        ', registry.VAL_DOUBLE, journal.VRenum, adressen.Deb_Num, RDatum,' +
        ' GEGENKTO, journalpos.ArtNum, JournalPos.Bezeichnung'
      'from journalpos'
      'left join journal on journal.rec_id=journalpos.journal_id'
      'left join adressen on journal.Addr_id=adressen.rec_id'
      'left join artikel on journalpos.artikel_ID=artikel.REC_ID'
      'left join warengruppen on artikel.WARENGRUPPE=warengruppen.ID'
      
        'left join registry on journalpos.Steuer_code=registry.Name and r' +
        'egistry.Mainkey='#39'MAIN\\MWST'#39
      
        'where ((journal.quelle =3)) and Rdatum >= :DatVon and Rdatum <= ' +
        ':DatBis'
      'and journalpos.ARTIKELTYP not in('#39'X'#39')'
      'and journal.vrenum not like '#39'%STORNO%'#39
      'and registry.Val_Double is not NULL'
      'order by journal.vrenum')
    Params = <
      item
        DataType = ftUnknown
        Name = 'DatVon'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DatBis'
        ParamType = ptUnknown
      end>
    Left = 184
    Top = 550
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DatVon'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DatBis'
        ParamType = ptUnknown
      end>
  end
